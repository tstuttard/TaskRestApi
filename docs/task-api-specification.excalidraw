    {
      "id": "Yli5O1HyhOSCshh6dPsVv",
      "type": "text",
      "x": -106.09442100582953,
      "y": 764.1110702417352,
      "width": 832.03125,
      "height": 1656,
      "angle": 0,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "transparent",
      "fillStyle": "solid",
      "strokeWidth": 2,
      "strokeStyle": "solid",
